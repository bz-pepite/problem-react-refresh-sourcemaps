{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "module": "ESNext",
        "resolveJsonModule": true,
        "isolatedModules": true,
        //"noEmit": true,
        "emitDeclarationOnly": true,
    },
}